	"private": true,
	"main": "index.tsx",
	"repository": {
		"type": "git",
		"url": "https://github.com/davegillem/davegillem_com-react"
	},
	"keywords": [
		"javascript",
		"portfolio",
		"react",
		"scss",
		"typescript"
	],
	"license": "ISC",
	"homepage": ".",
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"babel": {
		"presets": [
			[
				"@babel/env",
				{
					"modules": false,
					"targets": {
						"browsers": [
							"last 2 versions"
						]
					}
				}
			],
			"@babel/react",
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run type-check && lint-staged"
		}
	},
	"scripts": {
		"build": "node scripts/build.js",
		"clean-cache": "npm cache clean --force",
		"clean-cache-verify": "npm cache verify",
		"format": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx)\"",
		"format:check": "npm run format -- --check",
		"format:fix": "npm run format -- --write",
		"lint": "eslint --ext .js,.ts,.tsx ./src",
		"lint:fix": "npm run lint -- --fix && npm run format:fix",
		"lint-rule-timings": "cross-env TIMING=1 npm run lint",
		"npmupdate": "shx rm -rf node_modules package-lock.json yarn.lock && npm install",
		"start": "node scripts/start.js",
		"test": "npm run lint",
		"type-check": "tsc"
	},
